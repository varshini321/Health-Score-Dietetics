                <thead>
                <tr style="color: black;font-weight: bold;text-align:center" class='numeric center-text'">
                <td class='numeric'> Appointment ID</td>
                <td class='numeric'> Patient Name</td>
                <td class='numeric'> Patient Email</td>
                <td class='numeric'> Mobile</td>
                <td class='numeric'> Date</td>
                <td class='numeric'> Time</td>
                <td class='numeric'> Problem</td>
                <td class='numeric'>Status</td>
                </tr>
                </thead>
                <tbody>
                    <%  print.forEach(function(result) { %>
                        <tr style="color: black;text-align:center" class='numeric center-text'>
                            <td><%= result.aid %></td>
                            <td><%= result.patientname %></td>
                            <td><%= result.patientemail %></td>
                            <td><%= result.patientmobile %></td>
                            <td><%= result.adate %></td>
                            <td><%= result.atime %></td>
                            <td><%= result.problem %></td>
                            <td ><%= result.status %></td>
                            <td>
                                <div class='btn-group'>
                                    <% if(result.status=="Allow") 
                                    { %>
                                            Allowed
                                            <br><br>
                                            <a class='btn btn-danger' href='/denyappointment?aid=<%= result.aid %>'>Deny</a>        
                                    <% }
                                    else if(result.status=="Denied")
                                    {
                                        %>

                                    <a class='btn btn-success' href='/allowappointment?aid=<%= result.aid %>'>Allow</a>
                                    <br><br>
                                    Denied
                                    <% } else {
                                        %>
                                        <a class='btn btn-success' href='/allowappointment?aid=<%= result.aid %>'>Allow</a>
                                        <br><br>               
                                        <a class='btn btn-danger' href='/denyappointment?aid=<%= result.aid %>'>Deny</a>            
                                     <% } %>   
                                </div>
                            </td>
                        </tr>
                    <% }) %>
                    
